{
  "name": "leafletjs-svelte",
  "version": "0.0.1",
  "description": "Svelte component for LeafletJS with annotation plugins",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build package": "vite build && npm run package",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.59.1"
  },
  "devDependencies": {
    "@fontsource-variable/jetbrains-mono": "^5.0.1",
    "@fontsource-variable/nunito": "^5.0.0",
    "@fontsource-variable/overpass": "^5.0.1",
    "@fontsource/dejavu-mono": "^5.0.0",
    "@fontsource/fira-mono": "^5.0.1",
    "@fontsource/jetbrains-mono": "^5.0.0",
    "@playwright/test": "^1.34.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.18.0",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.28.0",
    "mdsvex": "^0.10.6",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "shiki": "^0.14.2",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vitest": "^0.31.1"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@types/leaflet": "^1.9.3",
    "leaflet": "^1.9.4"
  },
  "keywords": [
    "svelte",
    "leaflet"
  ]
}
